{
  "name": "imgql-syntax-highlighting",
  "displayName": "ImgQL Syntax Highlighting",
  "description": "Syntax highlighting support for ImgQL (VoxLogicA)",
  "icon": "imgs/logo.png",
  "version": "1.0.0",
  "publisher": "CoffeeStraw",
  "repository": {
    "type": "git",
    "url": "https://github.com/CoffeeStraw/ImgQL-Syntax-Highlighting-VsCode"
  },
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "imgql",
      "aliases": ["ImgQL", "imgql"],
      "extensions": ["imgql"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "imgql",
      "scopeName": "source.imgql",
      "path": "./syntaxes/imgql.tmLanguage.json"
    }]
  }
}
